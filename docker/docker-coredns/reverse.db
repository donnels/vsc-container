$ORIGIN 20.172.in-addr.arpa.
$TTL 300

@               IN  SOA coredns.vsagcrd.org. admin.vsagcrd.org. (
                    2025081001 ; serial
                    3600       ; refresh
                    1800       ; retry
                    1209600    ; expire
                    300        ; minimum
                )

                IN  NS  coredns.vsagcrd.org.

; Reverse DNS entries
10.0            IN  PTR coredns.vsagcrd.org.
11.0            IN  PTR proxy.vsagcrd.org.
12.0            IN  PTR vscode.vsagcrd.org.
13.0            IN  PTR certbot.vsagcrd.org.
