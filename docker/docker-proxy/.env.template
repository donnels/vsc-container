# Forward Proxy configuration
PROXY_DOMAIN=forward-proxy.local

# HAProxy configuration
HAPROXY_STATS_ENABLED=false
HAPROXY_STATS_PORT=8404

# LCARS interface settings
LCARS_ENABLED=true
LCARS_PORT=8080

# Self-signed certificate settings
CERT_COUNTRY=US
CERT_STATE=Development
CERT_CITY=Local
CERT_ORG=VSCode Container
CERT_UNIT=Development
CERT_COMMON_NAME=forward-proxy.local

# Proxy timeouts (in milliseconds)
PROXY_CONNECT_TIMEOUT=5000
PROXY_CLIENT_TIMEOUT=50000
PROXY_SERVER_TIMEOUT=50000
PROXY_TUNNEL_TIMEOUT=3600000

# Note: No PROXY_CERT_NAME needed - using self-signed certificate
