# Example .env file for certbot container
DOMAIN=vsagcrd.org
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token
CLOUDFLARE_ZONE_ID=your_zone_id (optional)

# VS Code certificate (for backend service - only LE cert needed)
VSCODE_DOMAINS=code-dev.vsagcrd.org,vscode.vsagcrd.org,ide.vsagcrd.org
VSCODE_CERT_NAME=vscode

# Generate only VSCode certificate (proxy uses self-signed)
GENERATE_PROXY_CERT=false
GENERATE_VSCODE_CERT=true
